>>>>>>> dev

ans1 = 0
ans2 = 0
ans3 = 1
ans4 = 1
for line in sys.stdin:
    try:
        num = int(line)
        ans1 += num
        ans2 -= num
        ans3 *= num
        ans4 /= num
    except:
        num = float(line)
        ans1 += num
        ans2 -= num
        ans3 *= num
        ans4 /= num

lst1 = list(range(1, num + 1))
print(lst1, "=", ans1)

lst2 = list(range(-num, num - num))
print(lst2, "=", ans2)

lst3 = list(range(1, num + 1))
print(lst3, "=", ans3)

lst4 = list(range(1, num + 1))
print(lst4, "=", f'{ans4:.10f}')
